{
  "author": "Ben Newman <bn@cs.stanford.edu>",
  "name": "@gerhobbelt/recast",
  "version": "0.18.1-33",
  "description": "JavaScript syntax tree transformer, nondestructive pretty-printer, and automatic source map generator",
  "keywords": [
    "ast",
    "rewriting",
    "refactoring",
    "codegen",
    "syntax",
    "transformation",
    "parsing",
    "pretty-printing"
  ],
  "homepage": "http://github.com/benjamn/recast",
  "repository": {
    "type": "git",
    "url": "git://github.com/GerHobbelt/recast.git"
  },
  "license": "MIT",
  "main": "main.js",
  "types": "main.d.ts",
  "scripts": {
    "tsc": "tsc --noEmit",
    "mocha": "bash -c test/run.sh",
    "debug": "bash -c -- test/run.sh --inspect-brk",
    "test": "npm run build && npm run mocha",
    "build": "npm run clean && tsc && ts-add-module-exports",
    "clean": "ts-emit-clean",
    "prepack": "npm run clean && npm run build",
    "postpack": "npm run clean",
    "pub": "npm publish --access public"
  },
  "browser": {
    "fs": false
  },
  "dependencies": {
    "@gerhobbelt/babel-parser": "7.0.0-beta.49",
    "@gerhobbelt/ast-types": "0.13.2-29",
    "@gerhobbelt/esprima": "4.0.1-27",
    "core-js": "3.1.4",
    "private": "0.1.8",
    "source-map": "0.7.3"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/parser": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@types/esprima": "4.0.2",
    "@types/glob": "7.1.1",
    "@types/mocha": "5.2.7",
    "@types/node": "12.6.8",
    "esprima-fb": "15001.1001.0-dev-harmony-fb",
    "flow-parser": "0.103.0",
    "glob": "7.1.4",
    "mocha": "6.2.0",
    "reify": "0.20.12",
    "semver": "6.2.0",
    "ts-add-module-exports": "1.0.0",
    "ts-emit-clean": "1.0.0",
    "ts-node": "8.3.0",
    "typescript": "3.5.3"
  },
  "engines": {
    "node": ">= 8.0"
  }
}
